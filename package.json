{
  "name": "@clawfi/sdk",
  "version": "1.0.0",
  "description": "Official SDK for the ClawFi crypto intelligence API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "clawfi",
    "crypto",
    "signals",
    "defi",
    "token-analysis",
    "blockchain",
    "ethereum",
    "solana"
  ],
  "author": "ClawFi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClawFiAI/clawfi-sdk.git"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
